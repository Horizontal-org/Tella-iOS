	<array>
		<string>OpenSans-Italic.ttf</string>
		<string>OpenSans-Regular.ttf</string>
		<string>OpenSans-Bold.ttf</string>
		<string>OpenSans-Light.ttf</string>
		<string>Roboto-Light.ttf</string>
		<string>OpenSans-Semibold.ttf</string>
	</array>
